<table>
    <thead>
        <tr>
            <th>id</th>
            <th>obraz</th>
            <th>name</th>
            <th>size</th>
            <th>type</th>
            <th>-</th>
            <th>-</th>
            <th>-</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{id}}</td>
            <td>
                <img width="40" height="40" src="{{getImage type}}">
            </td>
            <td>{{name}}</td>
            <td>{{size}}</td>
            <td>{{type}}</td>
            <td><a href="/delete?id={{id}}">DELETE</a></td>
            <td><a href="/info?id={{id}}">INFO</a></td>
            <td><a href="/download?id={{id}}">DOWNLOAD</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
